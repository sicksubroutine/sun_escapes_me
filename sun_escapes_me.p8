function _init()
	cls()
	ft={}
	initfont()
	poke(0x5f2c,3)
	t=0
	startscreen()
	shake=0
	blinkt=1
	cartdata("sun_escapes_me")
	
	sun={}
	
	sun.sprsx=0
	sun.sprsy=32
	sun.sprw=16
	sun.sprh=32
	sun.x=-5
	sun.y=0
	sun.dw=32
	sun.dh=64
	sun.aniframe=64
	sun.ani={0,16,32,16}
	sun.anispd=0.25
end

function _update()
	blinkt+=1
	if mode=="start" then
		update_start()
	elseif mode=="game" then
		update_game()
	elseif mode=="over" then
		update_over()
	end
end

function _draw()

	doshake()

	if mode=="start" then
		draw_start()
	elseif mode=="game" then
		draw_game()	
	elseif mode=="over" then
		draw_over()
	end
	
	camera()
end

function startgame()
	mode="game"
	
	p=makespr()
	
	p.x=50
	p.y=25
	p.sx=0
	p.sy=0
	p.spr=32
	p.colw=6
	p.colh=3
	p.ded=false
	
	flame_spr=3
	
	t=0
	friction=0.65
	gravity=0.2
	gravity2=0
	score=0
	
	explode={}
	
	parts={}
	parts2={}
	stars={}
	for i=1,50 do
		local newstar={}
		newstar.x=flr(rnd(64))
		newstar.y=flr(rnd(64))
		newstar.spd=rnd(1.75)+0.20
		add(stars,newstar)
	end
	
	nebula={}
	rocks={}
	delay=0
	knockback=false
	btn_release=0
	wait=0
	wait2=0
	buls={}
	btimer=10
	muzzle=0
	hiscore=dget(0)
	invul=30
	floats={}
	
end

function startscreen()
	mode="start"
	stars={}
	for i=1,50 do
		local newstar={}
		newstar.x=flr(rnd(64))
		newstar.y=flr(rnd(64))
		newstar.spd=rnd(1.75)+0.20
		add(stars,newstar)
	end
	parts={}
end	

