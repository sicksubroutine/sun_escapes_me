--draw function

function draw_game()
	cls()
	starfield()
	
	for i=0,15 do
		line(0+i,0+i,0+i,64,9)
	end
	
	--draw player ship	
	spr(p.spr,p.x,p.y)
	spr(flame_spr,p.x-5,p.y)
	
	--print(p.x,0,25)
	print("dead? ",40,0,8)
	print(p.ded)
	
	for par in all (parts) do
		local pc=7
		pc=page_red(par.age)
			
		circfill(par.x,par.y,par.size,pc)
		par.x+=par.sx
		par.y+=par.sy
		par.sx=par.sx*0.7
		par.sy=par.sy*0.7
		par.age+=1
		if par.age>par.maxage then
			par.size-=0.5
			if par.size<0 then
				del(parts,par)
			end
		end
	end
	
	for myrocks in all(rocks) do
	
		if myrocks.col=="red" or myrocks.col==nil then
			pal(13,2)
			pal(6,8)
			spr(2,myrocks.x,myrocks.y)
			pal()
		elseif myrocks.col=="blue" then
			pal(13,1)
			pal(6,12)
			spr(2,myrocks.x,myrocks.y)
			pal()
		elseif myrocks.col=="og" then
			pal()
			spr(2,myrocks.x,myrocks.y)
			pal()
		elseif myrocks.col=="green" then
			pal(13,3)
			pal(6,11)
			spr(2,myrocks.x,myrocks.y)
			pal()
		end	 	 	
	end
	
	print("score:"..score,8,0,7)
end

function draw_start()
	
	for x=1,10 do
		circfill(30,30,1+x,9)
	end
end
