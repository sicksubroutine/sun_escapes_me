function _init()
	cls()
	poke(0x5f2c,3)
	t=0
	startscreen()
end

function _update()
	t+=1
	if mode=="start" then
		update_start()
	elseif mode=="game" then
		update_game()
	elseif mode=="over" then
	end
end

function _draw()
	if mode=="start" then
		draw_start()
	elseif mode=="game" then
		draw_game()	
	elseif mode=="over" then
	end
end

function startgame()
	mode="game"
	
	p={} 
	
	p.x=25
	p.y=25
	p.sx=0
	p.sy=0
	p.spr=32
	p.ded=false
	
	flame_spr=3
	
	friction=0.65
	gravity=0.25
	score=0
	
	explode={}
	
	parts={}
	stars={}
	for i=1,50 do
		local newstar={}
		newstar.x=flr(rnd(64))
		newstar.y=flr(rnd(64))
		newstar.spd=rnd(1.75)+0.20
		add(stars,newstar)
	end
	
	nebula={}
	rocks={}
	delay=0
	knockback=false
	btn_release=0
end

function startscreen()
	mode="start"
	stars={}
	for i=1,50 do
		local newstar={}
		newstar.x=flr(rnd(64))
		newstar.y=flr(rnd(64))
		newstar.spd=rnd(1.75)+0.20
		add(stars,newstar)
	end
end	



