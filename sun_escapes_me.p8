function move(obj)
	obj.x+=obj.sx
	obj.y+=obj.sy
end

function animate(thi)
	thi.aniframe+=thi.anispd
		if flr(thi.aniframe)>#thi.ani then
			thi.aniframe=1
		end
		if thi.side==nil then
			thi.spr=thi.ani[flr(thi.aniframe)]
		elseif thi.side=="left" or thi.side=="right" then	
			thi.sprsx=sun.ani[flr(thi.aniframe)]
		end	
end

