	if btn(üÖæÔ∏è) then
		music(-1,100)
		sfx(3)	
		startgame()
	end
end

function update_cut1()
	animstars()
	if t>0 and t<1.1 then
		scnbegin=t
		music(11)
	end
	animate(sun)
	if phase=="phase1" then
		shake=4
		
		if scnbegin+2*30<t then
			scnbegin=t
			phase="phase2"
		end
	elseif phase=="phase2" then
		srtbtn()
		if scnbegin+2*30<t then
			scnbegin=t
			phase="phase3"
		end
	elseif phase=="phase3" then
		srtbtn()
		if scnbegin+0.7*30<t then
			scnbegin=t
			p.spr=48
			phase="phase4"
		end
	elseif phase=="phase4" then
		p.x-=1.5
		srtbtn()
		if scnbegin+2*30<t then
			scnbegin=t
			p.y=20
			p.x=-10
			p.spr=48
			phase="phase5"
		end
	elseif phase=="phase5"	then
		p.spr=32
		p.x+=1.5
		srtbtn()
		if scnbegin+2*30<t then
			scnbegin=t
			p.x=40
			p.y=28
			phase="phase6"
		end	
	elseif phase=="phase6" then
		p.spr=32	
		srtbtn()
		phase="phase6"	
		if btn(‚¨ÜÔ∏è) then
			p.y-=1
			p.spr=16
		end
		if btn(‚¨áÔ∏è) then
			p.y+=1
			p.spr=33
		end
		if btn(‚¨ÖÔ∏è) then
			p.x-=1
			p.spr=48
		end
		if btn(‚û°Ô∏è) then
			p.x+=1	
		end
		btimer-=1
		if btimer<0 then	
			if btn(‚ùé) then
				local newbul=makespr()
				newbul.x=p.x+1
				newbul.y=p.y
				newbul.spr=35
				newbul.colw=5
				newbul.colh=3
				newbul.invul=5
				newbul.rev=false
				add(buls,newbul)
				btimer=10
				muzzle=6
				sfx(6)
			end
		end
		for mybul in all(buls) do
			mybul.x+=3
		end	
		if p.x<0 then
			p.x=0
		end
		if p.y>60 then
			p.y=60
		end
		
		if p.y<0 then
			p.y=0
		end		
		
		if p.x>58 then
			p.x=58
		end	
		
	end
	
end

