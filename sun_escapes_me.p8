function _update()
	blinkt+=1
	t+=1
	if mode=="start" then
		update_start()
	elseif mode=="cut1" then
		update_cut1()
	elseif mode=="game" then
		update_game()
	elseif mode=="over" then
		update_over()
	end
end

function _draw()

	doshake()

	if mode=="start" then
		draw_start()
	elseif mode=="cut1" then
		draw_cut1()	
	elseif mode=="game" then
		draw_game()	
	elseif mode=="over" then
		draw_over()
	end
	print(debug,40,50,8)
	camera()
end

