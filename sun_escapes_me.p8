function update_game()	
	animstars()
	gravity+=0.01/100
	--reset values
	t+=1
	if wait2==0 then
	 gravity2=0
	end
	p.sy=0
	p.sx=0
	p.spr=32
	if knockback==true and t%25==0 then
		knockback=false		
	end
	if btn(‚¨ÜÔ∏è) then
		p.sy-=0.8
		p.spr=16
		p.sx-=0.03
		sfx(1)
	end 
	if btn(‚¨áÔ∏è) then
		p.sy+=0.8
		p.spr=33
		p.sx-=0.03
		sfx(1)
	end 
	if btn(‚¨ÖÔ∏è) then
		p.sx-=0.5
		p.spr=48
		
	end
	
	if knockback==false then
		if btn(‚û°Ô∏è) then
			p.sx+=0.75
			sfx(1)
		end 
	end
	
	if btimer<0 then
		if btn(‚ùé) and p.ded==false then
			local newbul=makespr()
			newbul.x=p.x+1
			newbul.y=p.y
			newbul.spr=35
			newbul.colw=5
			newbul.colh=3
			newbul.invul=5
			newbul.rev=false
			add(buls,newbul)
			btimer=10
			muzzle=6
			sfx(6)
		end
	end	
	btimer-=1
	wait-=1
	if p.x<9 and p.ded==false then
		p.ded=true
		wait=45
		shake=8
		explodes(p.x,p.y)
		p.y=100
		p.x=-100
		sfx(4)
		sfx(0)
	end
	
	if p.ded and wait==0 then
		mode="over"
	end	
	
	p.sx-=gravity+gravity2
	p.sy*=friction
	
	p.x=p.x+p.sx
	p.y=p.y+p.sy
	
	if muzzle>0 then
		muzzle-=2
	end
	

	
	flame_spr=flame_spr+1
	if flame_spr>5 then
		flame_spr=3
	end
	
	for myrocks in all(rocks) do
		myrocks.x-=0.5
		if myrocks.x<5 then
			if myrocks.x<p.x then
				score=score+1
			end	
			explodes(myrocks.x,myrocks.y)
			sfx(0)
			del(rocks,myrocks)
		end	
	end

	for myneb in all(nebula) do
		myneb.x-=0.5
		if myneb.x<5 then
			explodes(myneb.x,myneb.y)
			del(nebula,myneb)
			sfx(2)
		end
		myneb.spr+=0.25
		if myneb.spr==20 then
			myneb.spr=18
		end			
	end	

	--spawn code
	for x=1,1+t/250 do
		if #nebula<(rnd(t/250)) then
			newneb()
		end
	end
		
	for x=1,1+t/250 do
		if #rocks<(rnd(t/250)) then
			newrock()
		end
	end
	
	
	--bullet code
	for mybul in all(buls) do
		mybul.x+=3
			if mybul.bounce==true then
				mybul.x-=5
				mybul.rev=true
			end
			if mybul.x<5 then
				explodes(mybul.x,mybul.y)
				del(buls,mybul)
				sfx(0)
			end	
	end
	--bullet collision code
	for mybul in all(buls) do
		for myrock in all(rocks) do
			if col(mybul,myrock) then
				if myrock.hp>0.01 then
					myrock.hp-=1
					myrock.flash=3
					sparks(mybul.x,mybul.y)
					sfx(2)
					del(buls,mybul)
				end
				
				if myrock.hp<=1 then
					del(buls,mybul)
					del(rocks,myrock)
					explodes(myrock.x,myrock.y)
					sfx(0)
					score+=2
					popfloat("+2",myrock.x,myrock.y)
				end	
			end	
		end	
	end
	
	for mybul in all(buls) do
		for myneb in all(nebula) do
			if col(mybul,myneb) then
				mybul.bounce=true
				myneb.flash=3
				sfx(5)
			end
		end	
	end
	--player collision
	invul-=1
	wait2-=1
	if invul<=0 then
		for mybul in all(buls) do
			mybul.invul-=1
			if mybul.invul<=0 then	
				if col(mybul,p) then
					sparks(p.x,p.y)
					del(buls,mybul)
					p.x-=5
					sfx(0)
					shake=5
					invul=30
				end
			end		
		end
	end
	
	if invul<=0 then
		for myrock in all(rocks) do
			if col(myrock,p) then
				sfx(0)
				shake=5
				p.x-=8
				score+=1
				--knockback=true
				explodes(myrock.x,myrock.y)
				del(rocks,myrock)
				invul=20
			end
		end
	end
	
	if invul<=0 then	
		for myneb in all(nebula) do
			if col(myneb,p) then
				gravity2=2.35
				wait2=8
				shake=3
				sfx(5)
				del(nebula,myneb)
				invul=15
			end
		end
	end	
	
	
		if p.x>54 then
			knockback=true
		end	
		
		if p.y>60 then
			p.y=60
		end
		
		if p.y<5 then
			p.y=5
		end		
		
		if p.x>60 then
			p.x=60
		end	
	
			animate(sun)
	
end

function animate(thi)
	thi.aniframe+=thi.anispd
		if flr(thi.aniframe)>#thi.ani then
			thi.aniframe=1
		end
			thi.sprsx=sun.ani[flr(thi.aniframe)]
end

function update_start()
	animstars()
	animate(sun)
	shake=0.25
	
	if btn(‚ùé) then
		sfx(3)
		for par in all (parts) do
			del(parts,par)
		end	
		startgame()
	end	
	
end

function update_over()
	mode="over"
	
	if score>hiscore then
		hiscore=score
	end
	
	dset(0,hiscore)
	
	if btn(‚ùé) then
		startgame()
	end
	
end
