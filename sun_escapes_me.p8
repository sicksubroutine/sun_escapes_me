			newbul.rev=false
			add(buls,newbul)
			btimer=10
			muzzle=6
			sfx(6)
		end
	end	
	btimer-=1
	wait-=1
	if p.x<1 and lvl=="boss" then
		p.x=1
	end	
	if p.x<9 and p.ded==false and lvl!="boss" then
		music(-1,100)
		p.ded=true
		wait=45
		shake=8
		explodes(p.x,p.y)
		p.x+=128
		sfx(4)
		sfx(0)
		
	elseif p.x>54 and p.ded==false and lvl=="boss" then
		music(-1,100)
		p.ded=true
		wait=45
		shake=8
		explodes(p.x,p.y)
		
		sfx(4)
		sfx(0)
		p.y=128
	end
	
	if p.ded and wait==0 then
		sun.side="left"
		mode="over"
	end	
	
	p.sx-=gravity+gravity2
	p.sy*=friction
	
	move(p)
	
	if muzzle>0 then
		muzzle-=2
	end
	
	for myrocks in all(rocks) do
		myrocks.x-=0.25
		if myrocks.x<5 then
			if myrocks.x<p.x then
				score=score+1
			end	
			explodes(myrocks.x,myrocks.y)
			sfx(0)
			del(rocks,myrocks)
		end	
	end

	for myneb in all(nebula) do
		myneb.x-=0.25
		if myneb.x<5 then
			explodes(myneb.x,myneb.y)
			del(nebula,myneb)
			sfx(2)
		end
		myneb.spr+=0.25
		if myneb.spr==20 then
			myneb.spr=18
		end			
	end	
	--bullet code
	for mybul in all(buls) do
		mybul.x+=3
			if mybul.bounce==true then
				mybul.x-=5
				mybul.rev=true
			end
			if mybul.x>64 then
				del(buls,mybul)
			end	
			if mybul.x<5 and lvl!="boss" then
				explodes(mybul.x,mybul.y)
				del(buls,mybul)
				sfx(0)
				sun.pain=35
				score+=5
				sfx(45)
				shake=6
				popfloat1("+5",0,14+rnd(24))
			elseif mybul.x>54 and lvl=="boss" then
				explodes(mybul.x,mybul.y)
				del(buls,mybul)
				sfx(0)
				sun.pain=15
				score+=5
				sfx(45)
				shake=6
				popfloat("+15",54,14+rnd(24))
			end
	end
	--bullet collision code
	for mybul in all(buls) do
		for myrock in all(rocks) do
			if col(mybul,myrock) then
				if myrock.hp>=0.01 then
					myrock.hp-=3
					myrock.flash=5
					sparks(mybul.x,mybul.y)
					sfx(2)
					del(buls,mybul)
				else
					del(buls,mybul)
					del(rocks,myrock)
					explodes(myrock.x,myrock.y)
					sfx(0)
					score+=3
					popfloat("+3",myrock.x,myrock.y)
				end	
			end	
		end	
	end
	
	for mybul in all(buls) do
		for myneb in all(nebula) do
			if col(mybul,myneb) then
				mybul.bounce=true
				myneb.flash=3
				sfx(5)
			end
		end	
	end
	--player collision
	invul-=1
	wait2-=1
	if invul<=0 then
		for mybul in all(buls) do
			mybul.invul-=1
			if mybul.invul<=0 then	
				if col(mybul,p) then
					sparks(p.x,p.y)
					del(buls,mybul)
					p.x-=5
					sfx(0)
					shake=5
					invul=30
				end
			end		
		end
	end
	
	if invul<=0 then
		for myrock in all(rocks) do
			if col(myrock,p) then
				sfx(0)
				shake=5
				p.x-=15
				score+=1
				knockback=true
				explodes(myrock.x,myrock.y)
				del(rocks,myrock)
				invul=20
			end
		end
	end
	
	if invul<=0 then	
		for myneb in all(nebula) do
			if col(myneb,p) then
				gravity2=1.5
				wait2=8
				shake=3
				sfx(5)
				del(nebula,myneb)
				invul=15
			end
		end
	end
	
	for myebuls in all(ebuls) do
			if col(myebuls,p) then
				shake=3
				p.ded=true
				music(-1,100)
		
				wait=45
				
				explodes(p.x,p.y)
				p.x+=128
				sfx(4)
				sfx(0)
				del(myebuls,ebuls)
				
			end
		end	
	
	if p.x>54 and p.ded==false then
		knockback=true
	end	
		
	if p.y>60 and p.ded==false then
		p.y=60
	end
		
		if p.y<5 and p.ded==false then
			p.y=5
		end		
		
		if p.x>60 and p.ded==false then
			p.x=60
		end	
	
			animate(sun)
	
	
	--level code

	boss()
	if lvl==1 then
		beg+=1
		rockme()
		if score>100 then
			
			nextlvl()
		end
	
	elseif lvl==2 then
		beg+=2
		nebme()
		
		if score>200 then
			
			nextlvl()
		end
	
	elseif lvl==3 then
		beg+=3
		rockme()
		nebme()
		
		if score>300 then
			gravity=-0.2
			sun.pain=25
			shake=10
			p.x=10
			explodes(sun.x,sun.y)
