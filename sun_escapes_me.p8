				newbul.spr=35
				newbul.colw=5
				newbul.colh=3
				newbul.invul=5
				newbul.rev=false
				add(buls,newbul)
				btimer=10
				muzzle=6
				sfx(6)
			end
		end
		for mybul in all(buls) do
			mybul.x+=3
		end	
		if p.x<14 then
			p.x=14
		end
		if p.y>60 then
			p.y=60
		end
		
		if p.y<0 then
			p.y=0
		end		
		
		if p.x>58 then
			p.x=58
		end	
		
	end
	
end
