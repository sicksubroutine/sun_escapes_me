function nextlvl()
	music(-1,100)
	
	sfx(8)
	beg=0
	lvldis=20
	rocks={}
	nebula={}
	music(0)
	lvl+=1
	
end

function boss()
	
	if sun.side=="right" and lvl==5 then
		subphs="boss"
		sun.x=35
		sun.rev=true
	elseif sun.side=="left" and lvl==4 then
		sun.rev=false
	elseif sun.side=="left" and lvl==0 then
		sun.rev=false
	elseif sun.side=="left" and lvl!=0 then
	 sun.x=-5
	 sun.rev=false	
	end	
end


