function cprint(txt,x,y,c)
	print(tosmall(txt),x-#txt*2,y,c)
end

function fire(ang,spd)
	local myebul=makespr()
	myebul.x=54+rnd(3)
	myebul.y=rnd(32)
	myebul.spr=72
	myebul.aniframe=72
	myebul.ani={72,73,74,75}
	myebul.anispd=0.65
	
	myebul.sx=sin(ang)*spd
	myebul.sy=cos(ang)*spd
	
	myebul.colw=8
	myebul.colh=8

	add(ebuls,myebul)

	return myebul	
end

function ext(mynum)
	if subphs!="boss" then
		local digit=sub(mynum,1,1)
		local digit2=sub(mynum,2,2)
		local digit3=sub(mynum,3,3)
		local digit4=sub(mynum,4,4)
		print(digit..digit2..digit3..digit4, 15,58,8)
	elseif subphs=="boss" then
		local digit=sub(mynum,2,2)
		local digit2=sub(mynum,3,3)
		local digit3=sub(mynum,4,4)
		local digit4=sub(mynum,5,5)
		print(digit..digit2..digit3..digit4, 15,58,8)
	end	
end


