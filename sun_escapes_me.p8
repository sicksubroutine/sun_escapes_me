--draw function

function draw_game()
	cls()
	starfield()
	
		sspr(sun.sprsx,sun.sprsy,sun.sprw,sun.sprh,sun.x,sun.y,sun.dw,sun.dh)
	--draw player ship
	
	if invul<=0 then
		drwmyspr(p)
		--spr(flame_spr,p.x-5,p.y)
	else	
		if sin(t/5)<0 then
				drwmyspr(p)
				--spr(flame_spr,p.x-5,p.y)
			end
	end
	

	for par in all (parts) do
		local pc=7
		pc=page_red(par.age)
			
		circfill(par.x,par.y,par.size,pc)
		par.x+=par.sx
		par.y+=par.sy
		par.sx=par.sx*0.7
		par.sy=par.sy*0.7
		par.age+=1
		if par.age>par.maxage then
			par.size-=0.5
			if par.size<0 then
				del(parts,par)
			end
		end
	end

	for mybul in all(buls) do
		drwmyspr(mybul)
	end	

	for myneb in all(nebula) do
		drwmyspr(myneb)
	end	

	for myrocks in all(rocks) do
		drwmyspr(myrocks)	 	 	
	end
	
	if muzzle>0 then
		circfill(p.x+5,p.y+1,muzzle,11)
	end	
	
	spr(11,5,0,3,1)
	pal(7,8)
	pal(6,2)
	dis_spr(score,1)
	dis_spr(hiscore,2)
	pal()
	spr(27,39,0)
end

function draw_start()
	cls()
	for par in all (parts) do
		local pc=7
		pc=page_sun(par.age)
			
		circfill(par.x,par.y,par.size,pc)
		par.x+=par.sx
		par.y+=par.sy
		par.sx=par.sx*0.7
		par.sy=par.sy*0.7
		par.age+=1
		if par.age>par.maxage then
			par.size-=0.5
			if par.size<0 then
				del(parts,par)
			end
		end
	end
	
	
	print("sun escapes me",3,8,8)
	print("sun splodin' ^",0,45,7)
	print("press x")
	print("to escape!")
end

function draw_over()
	cls()
	
	
	print("your high",0,0,3,1)
	print("score is:")
	print(hiscore,35,6,8)
	
	print("you died n00b!",0,25,8)
	print("press x to")
	print("try again!")

end
