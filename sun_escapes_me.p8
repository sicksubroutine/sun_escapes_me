function rockme()
	for x=1,1+beg/250 do
		if #rocks<(rnd(beg/250)) then
			newrock()
		end
	end
end

function nebme()
	
	for x=1,1+beg/300 do
		if #nebula<(rnd(beg/300)) then
			newneb()
		end
	end
end		

