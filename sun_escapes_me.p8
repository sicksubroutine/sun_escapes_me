--draw function

function draw_game()
	cls()
	starfield()
	
	
	if sun.pain>0 then
		pal(10,8)
		pal(7,2)
		sun.sprsx=sun.sprsx+16
	end
	
		
	sspr(sun.sprsx,sun.sprsy,sun.sprw,sun.sprh,sun.x,sun.y,sun.dw,sun.dh,sun.rev)
	pal()	
	--draw player ship
	
	if invul<=0 then
		drwmyspr(p)
		--spr(flame_spr,p.x-5,p.y)
	else	
		if sin(t/5)<0 then
				drwmyspr(p)
				--spr(flame_spr,p.x-5,p.y)
			end
	end
	

	for par in all (parts) do
		local pc=7
		pc=page_red(par.age)
			
		circfill(par.x,par.y,par.size,pc)
		par.x+=par.sx
		par.y+=par.sy
		par.sx=par.sx*0.7
		par.sy=par.sy*0.7
		par.age+=1
		if par.age>par.maxage then
			par.size-=0.5
			if par.size<0 then
				del(parts,par)
			end
		end
	end

	for mybul in all(buls) do
		drwmyspr(mybul)
	end	
	
	
	for myneb in all(nebula) do
		myneb.flash-=1
		if myneb.flash>0 then
			for i=1,15 do
				pal(i,7)
			end
		end	
		drwmyspr(myneb)
		pal()
		
	end	
	
	for myrocks in all(rocks) do
			myrocks.flash-=1
		if myrocks.flash>0 then
			for i=1,15 do
				pal(i,7)
			end
		end	
		drwmyspr(myrocks)	
		pal() 	 	
	end
	
	if muzzle>0 then
		circfill(p.x+5,p.y+1,muzzle,11)
	end	
	
	for myp in all (parts2) do
		local pc=page_spark(myp.age)
			
		circfill(myp.x,myp.y,myp.size,pc)
		myp.x+=myp.sx
		myp.y+=myp.sy
		myp.sx=myp.sx*0.7
		myp.sy=myp.sy*0.7
		myp.age+=1
		if myp.age>myp.maxage then
			myp.size-=0.5
			if myp.size<0 then
				del(parts2,myp)
			end
		end
	end
	
	
	for myebul in all (ebuls) do
		
		drwmyspr(myebul)
		
	end	
	
	spr(11,0,0,3,1)
	pal(7,8)
	pal(6,2)
	dis_spr(score,1)
	dis_spr(hiscore,2)
	pal()
	spr(27,37,0)
	
	for myfl in all(floats) do
		local mycol=7
		if t%4<2 then
			mycol=8
		end 	
		cprint(myfl.txt,myfl.x,myfl.y,mycol)
		myfl.x-=0.5
		myfl.age+=1
		if myfl.age>40 then
			del(floats,myfl)
		end	
	end
	
	for myfl in all(floats1) do
		local mycol=7
		if t%4<2 then
			mycol=8
		end 	
		cprint(myfl.txt,myfl.x,myfl.y,mycol)
		myfl.x+=0.5
		myfl.age+=1
		if myfl.age>40 then
			del(floats1,myfl)
		end	
	end
	
	cprint("g",10,58,8)
	print(gravity, 15,58,8)
	
	
	if lvldis>19 then
		popfloat("nxt lvl!",50,14+rnd(16))
	elseif lvldis<19 then
	end
	
end

