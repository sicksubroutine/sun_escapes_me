		p.sy-=0.8
		p.spr=16
		p.sx-=0.03
		sfx(1)
	end 
	if btn(‚¨áÔ∏è) then
		p.sy+=0.8
		p.spr=33
		p.sx-=0.03
		sfx(1)
	end 
