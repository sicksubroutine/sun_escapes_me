--update functions
function update_game()	
	animstars()
	--reset values
	t=0
	gravity=0.25
	p.sy=0
	p.sx=0
	p.spr=32
	
	if knockback==true and t%20==0 then
		knockback=false		
	end
	if btn(‚¨ÜÔ∏è) then
		p.sy-=1
		p.spr=16
		p.x-=0.03
		sfx(1)
	end 
	if btn(‚¨áÔ∏è) then
		p.sy+=1
		p.spr=48
		p.x-=0.03
		sfx(1)
	end 
	if btn(‚¨ÖÔ∏è) then
		p.sx-=1
	end
	
	 if knockback==false then
			if btn(‚û°Ô∏è) then
				p.sx+=1
				sfx(1)
			end 
		end
		
	
	if p.x<16 then
		p.ded=true
		explodes(p.x,p.y)
		p.y=100
		p.x=-100
	end

	
	p.sx-=gravity
	p.sy*=friction
	
	
	p.x=p.x+p.sx
	p.y=p.y+p.sy
	
	if p.x>56 then
		p.x=56
	end	
	flame_spr=flame_spr+1
	if flame_spr>5 then
		flame_spr=3
	end
	
	for myrocks in all(rocks) do
		myrocks.x-=1.5
		if myrocks.x<5 then
			if myrocks.x<p.x then
				score=score+1
			end	
			explodes(myrocks.x,myrocks.y)
			sfx(0)
			del(rocks,myrocks)
		end	
	end
	--[[
	if t<120 then 
	
		if #rocks<2 then
			newrock()
		end
	end
	
	if t>121 then 
	
		if #rocks<4 then 
			newrock()
		end
	end		
	]]--
	
	for x=1,25 do
	
		if t%100==0 then
			x+=1
		end	
		if #rocks<x-19 then
			newrock()
		end
	end
	
		for myrock in all(rocks) do		
			if col(myrock,p) then
				sfx(0)
				p.x-=7
				score+=1
				knockback=true
				explodes(myrock.x,myrock.y)
				del(rocks,myrock)
			end
		end
		
		if p.x>56 then
			knockback=true
		end	
		
end

function update_start()
	animstars()
	
	if btn(‚ùé) then
		sfx(3)
		startgame()
	end	
	
end
